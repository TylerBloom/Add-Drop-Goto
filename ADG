_adg()
{
	# define variables
	local nums cur JUMPS latest DIRS word

  # Removes the space in the auto-complete
  compopt -o nospace
	
	# Setting COMPREPLY to be an empty array as nothing should be passed to it
	COMPREPLY=()
	
	# Setting cur to be the word the cursor is currently on
	cur="${COMP_WORDS[COMP_CWORD]}"
  
  # Calls the auto-complete python script
  COMPREPLY=( $(compgen -W "$(~/.waypoints/autoComplete.py ${COMP_WORDS[@]})" -- ${cur}) )
	if [ ${#COMPREPLY[@]} == 1 ]; then if [ "${COMPREPLY[0]}" == "* " ]; then COMPREPLY=${COMPREPLY[0][::-1]}; fi; fi 

	return 0
} 
complete -F _adg adg
